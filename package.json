{
  "name": "ai2sql",
  "displayName": "AI2SQL",
  "description": "AI-powered SQL query generator and optimizer in VSCode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:ai2sql-sidebar"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "ai2sql-sidebar-view",
          "title": "AI2SQL",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "ai2sql-sidebar-view": [
        {
          "type": "webview",
          "id": "ai2sql-sidebar",
          "name": "AI2SQL",
          "icon": "media/icon.svg",
          "contextualTitle": "AI2SQL"
        }
      ]
    },
    "commands": [
      {
        "command": "ai2sql.generateSQL",
        "title": "AI2SQL: Generate SQL from Natural Language"
      },
      {
        "command": "ai2sql.optimizeSQL",
        "title": "AI2SQL: Optimize SQL Query"
      }
    ],
    "configuration": {
      "title": "AI2SQL",
      "properties": {
        "ai2sql.provider": {
          "type": "string",
          "enum": ["openai", "claude", "deepseek"],
          "default": "openai",
          "description": "The AI provider to use for SQL generation and optimization"
        },
        "ai2sql.openaiApiKey": {
          "type": "string",
          "default": "",
          "description": "Your OpenAI API key"
        },
        "ai2sql.openaiModel": {
          "type": "string",
          "default": "gpt-4",
          "description": "The OpenAI model to use (e.g. gpt-4, gpt-3.5-turbo)"
        },
        "ai2sql.claudeApiKey": {
          "type": "string",
          "default": "",
          "description": "Your Anthropic Claude API key"
        },
        "ai2sql.claudeModel": {
          "type": "string",
          "default": "claude-3-opus-20240229",
          "description": "The Claude model to use (e.g. claude-3-opus-20240229, claude-3-sonnet-20240229)"
        },
        "ai2sql.deepseekApiKey": {
          "type": "string",
          "default": "",
          "description": "Your Deepseek API key"
        },
        "ai2sql.deepseekModel": {
          "type": "string",
          "default": "deepseek-chat",
          "description": "The Deepseek model to use"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "20.x",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "openai": "^4.83.0"
  }
}
